{
  "extends": "@fullstacksjs/tsconfig/next",
  "compilerOptions": {
    "baseUrl": ".",
    "jsx": "preserve",
    "paths": {
      "@/*": [
        "./*"
      ]
    },
    "strictNullChecks": true
  },
  "include": [
    "next-env.d.ts",
    "**/*.ts",
    "**/*.tsx",
    "middleware.ts",
    ".next/types/**/*.ts",
    "tailwind.config.js"
  ],
  "exclude": [
    "node_modules",
    ".vscode",
    ".next"
  ]
}
